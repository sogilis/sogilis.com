{
  "scripts": {
    "sass:compile": "sass ./sass:./docs/fr/stylesheets",
    "sass:watch": "yarn sass:compile --watch",
    "browser-sync:run": "browser-sync start --server --watch --serveStatic './docs'",
    "dev": "run-p sass:watch browser-sync:run"
  },
  "devDependencies": {
    "browser-sync": "^2.27.3",
    "npm-run-all": "^4.1.5",
    "sass": "^1.35.1"
  }
}
